  },
  "dependencies": {
    "next": "15.4.5",
    "react": "^18",
    "react-dom": "^18",
    "firebase": "^10.7.1"
  },
  "devDependencies": {
    "eslint": "^8",
    "eslint-config-next": "15.4.5"
  }
