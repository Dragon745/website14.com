{
  "name": "website14app",
  "version": "3.1.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run generate-sitemap && next build",
    "start": "next start",
    "lint": "next lint",
    "generate-sitemap": "node scripts/generate-sitemap.js",
    "add-sample-blog": "node scripts/add-sample-blog-post.js",
